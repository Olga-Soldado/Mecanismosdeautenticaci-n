
<h1>Publications</h1>

<div id="publications">
  <% @publications.each do |publication| %>
    <%= render publication %>
    <p>
      <%= link_to "Show this publication", publication, class:'text' %>
    </p>
  <% end %>
</div>
<br>
<%= link_to "New publication", new_publication_path, class:'text' %>
